@model GestAgape.Core.Entities.Parametrage.Filiere

@{
    ViewData["Title"] = "Details série";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Module"] = "Paramétrage";

}

<div>
    <h4>série</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Libelle)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Libelle)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Code)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Code)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Departement)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Departement.Libelle)
        </dd>
    </dl>
</div>

<div>
    <a asp-action="UpdateFiliere" asp-route-id="@Model?.Id"> Modifier</a> |
    <a asp-action="Filiere">revenir en arriere</a>
</div>
