@model IEnumerable<GestAgape.Core.Entities.Scolarite.Inscription>

@{
    ViewData["Module"] = "GestIME";
    if (ViewBag.term == "nf")
    {
        ViewData["Title"] = "Aucun resultat pour votre recherche !!";

    }
    else
    {
        ViewData["Title"] = "Résultat de la recherche pour le mot-clé: '" + ViewBag.term + "'";

    }
    Layout = "~/Views/Shared/_Layout.cshtml";
}



