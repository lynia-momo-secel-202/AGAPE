@inject GestAgape.Service.Admissions.IAdmission admission
<div class="col-md-3 left_col" style="font-weight:bold;">
    <div class="menu_fixed scroll-view">
        <div class="clearfix"></div>
        <br />
        <!-- menu profile quick info -->
        <div class="profile clearfix">
            <div class="profile_pic">
                <img src="~/img/icons/Agape.png" class="mb-3" style="width: 120px; margin-left:65%;" alt="logo">
            </div>
        </div>

        <div class="clearfix">
            <div class="col-12" style="background: #fff; cursor: pointer; padding: 5px 10px; border: 1px solid #ccc;text-align:center; ">
                <label class="control-label sidebar-link"> Année en cours </label>
                <select class="form-control sidebar-link">
                    @foreach (var annee in admission.GetAllAnneeAcademique)
                    {
                        <option value="@annee.Id">@annee.AnneeDebut - @annee.AnneeFin</option>
                    }
                </select>
            </div>
        </div>
        <!-- /menu profile quick info -->

        <br />

        <!-- sidebar menu -->
        <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
            <div class="menu_section">
                <ul class="nav side-menu">
                    <li><a asp-action="Dashboard" asp-controller="Home"><i class="fa fa-home"></i> DASHBOARD</a></li>
                </ul>
