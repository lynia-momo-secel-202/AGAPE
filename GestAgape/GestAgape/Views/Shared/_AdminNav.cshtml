                            <li><a asp-action="EtatScolarite" asp-controller="Scolarite">Etat de scolarité</a></li>
                            <li><a asp-action="Etudiant" asp-controller="Scolarite">Paiement des frais de scolarité</a></li>
                            @*<li><a asp-action="Etudiant" asp-controller="Scolarite">Paiement des frais d'examen</a></li>
                            *@
                            <li><a asp-action="Bourse" asp-controller="Scolarite">Gestion des bourses</a></li>
                            <li><a asp-action="ChangementFiliere" asp-controller="Scolarite">Changements de spécialités</a></li>
                            <li><a asp-action="ChangementCampus" asp-controller="Scolarite">Changements de campus</a></li>
                        </ul>
                    </li>
                </ul>
                <ul class="nav side-menu">
